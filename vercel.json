{
  "buildCommand": "echo 'Using pre-built files'",
  "outputDirectory": "build",
  "installCommand": "echo 'No install needed'",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|json|ico|png|jpg|jpeg|svg|woff|woff2|txt))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
